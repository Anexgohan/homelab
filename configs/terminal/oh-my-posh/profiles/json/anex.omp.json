{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "bg_color": "#242424",
    "black": "#000000",
    "blue": "#0000ff",
    "cyan": "#00ffff",
    "dark_green": "#00a000",
    "dark_red": "#a00000",
    "error_color": "#e74856",
    "fg_color": "#ffffff",
    "frame_color1": "#2e9599",
    "frame_color2": "#0069af",
    "badge_color1": "#38305a",
    "badge_color2": "#1C508B",
    "git_ahead": "#999900",
    "git_behind": "#a54343",
    "green": "#00ff00",
    "magenta": "#ff00ff",
    "orange": "#ff7b00",
    "red": "#ff0000",
    "text_color1": "#0099ff",
    "text_color2": "#ffb055",
    "text_color3": "#ffe2c1",
    "white": "#ffffff",
    "yellow": "#ffff00"
  },
  "secondary_prompt": {
    "template": "❯❯ ",
    "foreground": "p:orange",
    "background": "transparent"
  },
  "transient_prompt": {
    "template": "❯ ",
    "background": "transparent",
    "foreground_templates": [
      "{{if gt .Code 0}}p:error_color{{end}}",
      "{{if eq .Code 0}}p:dark_green{{end}}"
    ]
  },
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "leading_diamond": "\ue0b6",
          "template": " \uf489 {{ .Name }} ",
          "foreground": "p:text_color3",
          "background": "p:badge_color1",
          "type": "shell",
          "style": "diamond"
        },
        {
          "options": {
            "always_enabled": true,
            "style": "austin",
            "threshold": 5000
          },
          "template": "<#5a5280>\ue0b1</> \uf520 {{ .FormattedMs }} ",
          "foreground": "p:text_color3",
          "background": "p:badge_color1",
          "type": "executiontime",
          "style": "powerline"
        },
        {
          "options": {
            "time_format": "Mon, Jan 02"
          },
          "powerline_symbol": "\ue0b0",
          "template": " \uf073 {{ .CurrentDate | date .Format }} ",
          "foreground": "p:text_color3",
          "background": "p:badge_color2",
          "type": "time",
          "style": "powerline"
        },
        {
          "options": {
            "time_format": "03:04:05 pm"
          },
          "template": "<#4a6a9a>\ue0b1</> \udb80\udc20 {{ .CurrentDate | date .Format }} ",
          "foreground": "p:text_color3",
          "background": "p:badge_color2",
          "trailing_diamond": "\ue0d1",
          "type": "time",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "options": {
            "style": "full"
          },
          "template": "<p:text_color2>\uf07c {{ .AbsolutePWD }}</>",
          "foreground": "p:text_color1",
          "background": "transparent",
          "type": "path",
          "style": "plain"
        },
        {
          "template": " ❯ ",
          "foreground": "p:orange",
          "background": "transparent",
          "type": "text",
          "style": "powerline"
        },
        {
          "template": "{{ if eq .OS \"windows\" }}<#0078D4>{{ .Icon }}[{{.OS}}]</>{{ else if eq .OS \"debian\" }}<#A80030>{{ .Icon }}[{{.OS}}]</>{{ else if eq .OS \"ubuntu\" }}<#E95420>{{ .Icon }}[{{.OS}}]</>{{ else if eq .OS \"arch\" }}<#1793D1>{{ .Icon }}[{{.OS}}]</>{{ else if eq .OS \"fedora\" }}<#51A2DA>{{ .Icon }}[{{.OS}}]</>{{ else if eq .OS \"centos\" }}<#932279>{{ .Icon }}[{{.OS}}]</>{{ else if eq .OS \"alpine\" }}<#0D597F>{{ .Icon }}[{{.OS}}]</>{{ else }}{{ .Icon }}[{{.OS}}]{{ end }}<p:orange,transparent>|</>{{.HostName}}<p:orange,transparent>|</>u:{{.UserName}}{{ if .Env.SSH_CONNECTION }}<p:orange,transparent>|</><p:yellow>SSH</>{{ end }}",
          "foreground": "p:text_color1",
          "background": "transparent",
          "type": "os",
          "style": "plain"
        },
        {
          "template": " ❯",
          "foreground": "p:orange",
          "background": "transparent",
          "type": "text",
          "style": "powerline"
        },
        {
          "options": {
            "commit_icon": "@",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ .UpstreamIcon }} {{ .HEAD }}{{ if gt .StashCount 0 }} \uf487 {{ .StashCount }}{{ end }}{{ if .Staging.Changed }} <p:green>stg: {{ add (add .Staging.Added .Staging.Modified) .Staging.Deleted }}</>{{ end }} <p:yellow>mod: {{ .Working.Modified }}</> <p:red>rem: {{ .Working.Deleted }}</> <p:cyan>untr: {{ .Working.Untracked }}</>{{ if gt .Ahead 0 }} [{{ .Ahead }}<p:git_ahead>\u2191</>]{{ end }}{{ if gt .Behind 0 }} [{{ .Behind }}<p:git_behind>\u2193</>]{{ end }}",
          "foreground": "p:text_color1",
          "powerline_symbol": "\ue0b0",
          "type": "git",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "❯",
          "background": "transparent",
          "type": "text",
          "style": "plain",
          "foreground_templates": [
            "{{if gt .Code 0}}p:error_color{{end}}",
            "{{if eq .Code 0}}p:dark_green{{end}}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
